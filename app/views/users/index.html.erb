<p id="notice"></p>

<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-3">
   <%= render 'books/list', user: @user, book: @book %>
  </div>

  <div class="col-md-8 offset-md-1">
<h2>Users</h2>

 <table>
     <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
     </tr>
     <% @users.each do |users| %>
     <tr>
      <td>
       <%= image_tag users.get_profile_image(100,100) %>
      </td>

      <td>
      <%= users.name %>
      </td>

      <td>
      <%= link_to "Show", user_path(users.id) %>
      </td>
     </tr>
     <% end %>
    </table>
   </div>
 </div>
</div>